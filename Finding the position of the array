//finding the position of the arrays
const items=[1,2,3,4,5,6,7,8];
const b=Number(prompt("Enter the value of your number"));

for(let i=0;i<items.length;i++){
    for(let j=i+1;i<items.length;j++){
        if(items[i]+items[j]===b){
            console.log(`My Index position is ${i}, and ${j}`);
        }
    }
}
